# Flask Secret Key
SECRET_KEY=your_secret_key_here

# Google Books API Key
# Get one at: https://developers.google.com/books/docs/v1/getting_started
GOOGLE_BOOKS_API_KEY=your_google_books_api_key_here

# Docker Environment (set to 1 if running in Docker)
DOCKER_ENV=0

# Cache TTL in seconds
TTL_SECONDS=60